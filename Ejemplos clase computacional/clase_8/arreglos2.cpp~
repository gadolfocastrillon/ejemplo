#include <iostream>

using namespace std;

int main(){

  char cadena1[20];
  char cadena2[]="literal de cadena";
	       
  cout<<"Escriba la cadena \"hola todos\":";
  cin>>cadena1;

  cout<<"cadena1 es:2 "<<cadena1<<"\ncandena2 es:"<<cadena2;

  cout<<"\nCadena1 con espacios entre caracteres es: \n";

  for(int i=0;cadena1[i]!='\0';i++){
    cout<<cadena1[i]<<' ';
  }

  cin>>cadena1;
  cout<<"\n candena 1 es:"<<cadena1<<endl;

  return 0;
}
      
  
